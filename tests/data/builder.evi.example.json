{
  "loadco1": {
    "process_id": "load_collection",
    "arguments": {
      "id": "Sentinel-2"
    }
  },
  "filter1": {
    "process_id": "filter_bbox",
    "arguments": {
      "data": {
        "from_node": "loadco1"
      },
      "extent": {
        "west": 16.1,
        "east": 16.6,
        "north": 48.6,
        "south": 47.2
      }
    }
  },
  "filter2": {
    "process_id": "filter_temporal",
    "arguments": {
      "data": {
        "from_node": "filter1"
      },
      "start": "2018-01-01",
      "end": "2018-02-01"
    }
  },
  "filter3": {
    "process_id": "filter_bands",
    "arguments": {
      "data": {
        "from_node": "filter2"
      },
      "bands": [
        "B08",
        "B04",
        "B02"
      ]
    }
  },
  "reduce1": {
    "process_id": "reduce",
    "arguments": {
      "data": {
        "from_node": "filter3"
      },
      "dimension": "spectral",
      "reducer": {
        "callback": {
          "arraye1": {
            "process_id": "array_element",
            "arguments": {
              "data": {
                "from_argument": "data"
              },
              "index": 0
            }
          },
          "arraye2": {
            "process_id": "array_element",
            "arguments": {
              "data": {
                "from_argument": "data"
              },
              "index": 1
            }
          },
          "arraye3": {
            "process_id": "array_element",
            "arguments": {
              "data": {
                "from_argument": "data"
              },
              "index": 2
            }
          },
          "substr1": {
            "process_id": "substract",
            "arguments": {
              "data": [
                {
                  "from_node": "arraye1"
                },
                {
                  "from_node": "arraye2"
                }
              ]
            }
          },
          "produc1": {
            "process_id": "product",
            "arguments": {
              "data": [
                6,
                {
                  "from_node": "arraye2"
                }
              ]
            }
          },
          "produc2": {
            "process_id": "product",
            "arguments": {
              "data": [
                -7.5,
                {
                  "from_node": "arraye3"
                }
              ]
            }
          },
          "sum1": {
            "process_id": "sum",
            "arguments": {
              "data": [
                1,
                {
                  "from_node": "arraye1"
                },
                {
                  "from_node": "produc1"
                },
                {
                  "from_node": "produc2"
                }
              ]
            }
          },
          "divide1": {
            "process_id": "divide",
            "arguments": {
              "data": [
                {
                  "from_node": "substr1"
                },
                {
                  "from_node": "sum1"
                }
              ]
            }
          },
          "produc3": {
            "process_id": "product",
            "arguments": {
              "data": [
                2.5,
                {
                  "from_node": "divide1"
                }
              ]
            },
            "result": true
          }
        }
      }
    }
  },
  "reduce2": {
    "process_id": "reduce",
    "arguments": {
      "data": {
        "from_node": "reduce1"
      },
      "dimension": "temporal",
      "reducer": {
        "callback": {
          "min1": {
            "process_id": "min",
            "arguments": {
              "data": {
                "from_argument": "data"
              }
            },
            "result": true
          }
        }
      }
    }
  },
  "export1": {
    "process_id": "export",
    "arguments": {
      "data": {
        "from_node": "reduce2"
      },
      "format": "GTiff"
    },
    "result": true
  }
}